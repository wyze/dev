    "react-router": "catalog:"
  },
  "devDependencies": {
    "@react-router/dev": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@wyze/typescript-config": "workspace:*",
    "@wyze/vite-config": "workspace:*",
    "typescript": "catalog:",
